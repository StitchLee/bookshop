<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bean">
	<class name="Temporary" table="temporary">
		<id name="orderId" type="int">
			<generator class="assigned" />
		</id>
		<property name="customerId" not-null="true" type="int">
		</property>
		<property name="orderTime" not-null="true" type="java.lang.String">
		</property>
		<property name="bookSet" not-null="true" type="java.lang.String">
		</property>
		<property name="bookAmonut" not-null="true" type="int">
		</property>
		<property name="isPay" not-null="true" type="int">
		</property>
		<property name="totalMoney" not-null="true" type="int">
		</property>
		<property name="customerAddr" not-null="true" type="java.lang.String">
		</property>
	</class>
</hibernate-mapping>
